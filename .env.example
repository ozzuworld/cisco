# CUCM Log Collector - Environment Variables (Example)
# Copy this file to .env and fill in your values for local development

# CUCM Connection (for testing)
CUCM_HOST=10.10.10.10
CUCM_PORT=22
CUCM_USERNAME=admin
CUCM_PASSWORD=your-password-here

# API Settings
API_HOST=0.0.0.0
API_PORT=8000
LOG_LEVEL=INFO

# Timeouts (seconds)
DEFAULT_CONNECT_TIMEOUT=30
DEFAULT_COMMAND_TIMEOUT=120
